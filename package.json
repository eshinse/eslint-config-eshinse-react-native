{
  "name": "eslint-config-eshinse-react-native",
  "version": "1.0.0",
  "description": "eshinse ESLint config for React Native.",
  "homepage": "https://github.com/eshinse/eslint-config-eshinse-react-native",
  "author": "Kenneth Sundqvist <kenneth@eshin.se> (https://eshin.se)>",
  "license": "Apache-2.0",
  "main": "index.yaml",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "release": "standard-version",
    "publish:github": "conventional-github-releaser -p angular"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eshinse/eslint-config-eshinse-react-native"
  },
  "bugs": {
    "url": "https://github.com/eshinse/eslint-config-eshinse-react-native/issues"
  },
  "keywords": [
    "eshinse",
    "eslint",
    "eslintconfig",
    "react",
    "native"
  ],
  "peerDependencies": {
    "eslint-plugin-react-native": "^2.2.0"
  },
  "devDependencies": {
    "conventional-github-releaser": "^3.1.2",
    "cz-conventional-changelog": "^1.2.0",
    "standard-version": "^4.0.0"
  }
}
